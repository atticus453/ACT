<mujoco>
    <include file="scene_tm12.xml"/>
    <include file="tm12_dependencies.xml"/>
	<include file="vx300s_dependencies.xml"/>

    <equality>
        <weld body1="mocap_left" body2="tm12_left/gripper_link" solref="0.01 1" solimp=".25 .25 0.001" />
        <weld body1="mocap_right" body2="tm12_right/gripper_link" solref="0.01 1" solimp=".25 .25 0.001" />
    </equality>


    <worldbody>
        <include file="tm12_right_g.xml" />
        <include file="tm12_left_g.xml" />
        <!-- -0.45 0.22 1.023 -->
        <!-- 0.017 0.39 0.37 -->
        <body mocap="true" name="mocap_left" pos="-0.45 0.22 1.023">
            <site pos="0 0 0" size="0.003 0.003 0.03" type="box" name="mocap_left_site1" rgba="1 0 0 1"/>
            <site pos="0 0 0" size="0.003 0.03 0.003" type="box" name="mocap_left_site3" rgba="0 1 0 1"/>
            <site pos="0 0 0" size="0.03 0.003 0.003" type="box" name="mocap_left_site2" rgba="0 0 1 1"/>
        </body>
        <body mocap="true" name="mocap_right" pos="0.469 0.78 1.023">
            <site pos="0 0 0" size="0.003 0.003 0.03" type="box" name="mocap_right_site1" rgba="1 0 0 1"/>
            <site pos="0 0 0" size="0.003 0.03 0.003" type="box" name="mocap_right_site2" rgba="1 0 0 1"/>
            <site pos="0 0 0" size="0.03 0.003 0.003" type="box" name="mocap_right_site3" rgba="1 0 0 1"/>
        </body>

        <body name="box" pos="0.0 0.6 0.05">
            <joint name="red_box_joint" type="free" frictionloss="0.01" />
            <inertial pos="0 0 0" mass="0.05" diaginertia="0.002 0.002 0.002" />
            <geom condim="4" solimp="2 1 0.01" solref="0.01 1" friction="1 0.005 0.0001" pos="0 0 0" size="0.02 0.02 0.02" type="box" name="red_box" rgba="1 0 0 1" />
        </body>

        <body name="box2" pos="0.0 0.4 0.05">
            <joint name="green_box_joint" type="free" frictionloss="0.01" />
            <inertial pos="0 0 0" mass="0.05" diaginertia="0.002 0.002 0.002" />
            <geom condim="4" solimp="2 1 0.01" solref="0.01 1" friction="1 0.005 0.0001" pos="0 0 0" size="0.02 0.02 0.02" type="box" name="green_box" rgba="0 1 0 1" />
        </body>

    </worldbody>

    <actuator>
		
        <position ctrllimited="true" ctrlrange="0.021 0.057" joint="tm12_left/left_finger" kp="200"  user="1"/>
        <position ctrllimited="true" ctrlrange="-0.057 -0.021" joint="tm12_left/right_finger" kp="200"  user="1"/>
		
        <position ctrllimited="true" ctrlrange="0.021 0.057" joint="tm12_right/left_finger" kp="200"  user="1"/>
        <position ctrllimited="true" ctrlrange="-0.057 -0.021" joint="tm12_right/right_finger" kp="200"  user="1"/>
        

    </actuator>

    <keyframe>
        <key qpos="0 0 1.57 0 1.57 0 0.024 -0.024  0 0 1.57 0 1.57 0  0.024 -0.024  0.0 0.6 0.05 1 0 0 0 0.0 0.6 0.05 1 0 0 0"/>
    </keyframe>


</mujoco>